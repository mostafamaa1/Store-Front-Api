{
  "name": "store-front-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier \"src/**/*.ts\" --check",
    "prettier:fix": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "start": "nodemon ./src/index.ts",
    "build": "npx tsc",
    "jasmine": "jasmine",
    "test": "db-migrate --config ./database.json --env test reset && set ENV=test && db-migrate --config ./database.json --env test up && npm run build && npm run jasmine",
    "start:prod": "npm run build && node build/index.js"
  },
  "keywords": [],
  "author": "Mostafa Ahmed",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/jasmine": "^4.3.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.23",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "bcrypt": "^5.0.1",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "express": "^4.18.1"
  }
}
